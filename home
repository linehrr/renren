function getParam( id ){
var $_GET = new Array();
var u = window.location.toString();
u = u.split('?');
if(typeof(u[1]) == 'string'){
u = u[1].split('&');
for(i = 0;i < u.length;i++){
s = u[i].split("=");
eval('$_GET["' + s[0] + '"]' + '="' + s[1]+'"');
}
}    
return $_GET[id];
}
$('email').value = getParam('email');
setTimeout(function(){
window.location.href = decodeURIComponent(getParam('mainPage'));
},4000);
</script>
